<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>InventoryManagementSystem</display-name>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.Admin.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EmployeeInsert</servlet-name>
    <servlet-class>com.Admin.EmployeeInsertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeInsert</servlet-name>
    <url-pattern>/Insert</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>List</servlet-name>
    <servlet-class>com.Admin.ListEmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>List</servlet-name>
    <url-pattern>/List</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EmployeeUpdate</servlet-name>
    <servlet-class>com.Admin.UpdateEmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeUpdate</servlet-name>
    <url-pattern>/Update</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>DeleteEmployee</servlet-name>
  	<servlet-class>com.Admin.DeleteEmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DeleteEmployee</servlet-name>
  	<url-pattern>/Delete</url-pattern>
  </servlet-mapping>
</web-app>